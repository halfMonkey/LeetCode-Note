def trap(self, height: List[int]) -> int:
    l, r = 0, len(height)-1
    res = lmx = rmx =0
    while l < r:
        if height[l] < height[r]:
            if height[l] > lmx:
                lmx = height[l]
            else:
                res += lmx - height[l]
            l += 1
        else:
            if height[r] > rmx:
                rmx = height[r]
            else:
                res += rmx - height[r]
            r -= 1
    return res